	echo 'F77 = f77' >>${srcdir}/makeinc
	echo "$as_me:$LINENO: result: Added definition of AR and F77 for HPUX in makeinc" >&5
echo "${ECHO_T}Added definition of AR and F77 for HPUX in makeinc" >&6
fi
echo '- Configure ready. Now edit makeinc, redefining at least all "unknown".'
echo '  Then type make.'
